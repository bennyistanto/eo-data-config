  "main_variables": ["SID", "SEASON", "NUMBER", "BASIN", "SUBBASIN", "NAME", "ISO_TIME", "NATURE", "LAT", "LON", "WMO_WIND", "WMO_PRES", "TRACK_TYPE", "DIST2LAND", "STORM_SPEED", "STORM_DIR"],
  "SID_description": "Storm Identifier",
  "SEASON_description": "Year of the cyclone season",
  "SEASON_units": "year",
  "NUMBER_description": "The cardinal number of the system for that season",
  "BASIN_description": "Basin in which the cyclone occurred",
  "BASIN_values": ["NA: North Atlantic", "EP: Eastern North Pacific", "WP: Western North Pacific", "NI: North Indian", "SI: South Indian", "SP: Southern Pacific", "SA: South Atlantic", "MM: Missing"],
  "SUBBASIN_description": "Sub-basin in which the cyclone occurred",
  "SUBBASIN_values": ["CS: Caribbean Sea", "GM: Gulf of Mexico", "CP: Central Pacific", "BB: Bay of Bengal", "AS: Arabian Sea", "WA: Western Australia", "EA: Eastern Australia", "MM: Missing"],
